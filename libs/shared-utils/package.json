{
  "name": "@workadventure/shared-utils",
  "version": "1.0.0",
  "description": "Map Editor commands types",
  "main": "./src/index.ts",
  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint-fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "precommit": "lint-staged",
    "pretty": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "pretty-check": "prettier --check 'src/**/*.{ts,tsx}' 'tests/**/*.{ts,tsx}'",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "NODE_OPTIONS='--loader tsx' node --inspect-brk -r tsconfig-paths/register node_modules/.bin/jest --runInBand",
    "typecheck": "tsc --noEmit",
    "build": "tsc"
  },
  "author": "WorkAdventure",
  "license": "ISC",
  "dependencies": {
    "@workadventure/map-editor": "1.0.0",
    "@workadventure/tiled-map-type-guard": "^2.1.2",
    "@grpc/grpc-js": "^1.8.12",
    "axios": "^1.3.2",
    "catch-unknown": "^2.0.0",
    "zod": "^3.23.8",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/node": "^18.11.18",
    "@types/debug": "^4.1.5",
    "@workadventure/eslint-config": "1.0.0",
    "eslint": "^9.25.1",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.3",
    "typescript": "^5.7.2"
  },
  "files": [
    "src"
  ],
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
