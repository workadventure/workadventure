{
  "private": true,
  "name": "map-storage",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "contributors": [
    {
      "name": "Grégoire Parant",
      "email": "gregoire@workadventu.re"
    },
    {
      "name": "David Négrier",
      "email": "david@workadventu.re"
    },
    {
      "name": "Piotr Hanusiak",
      "email": "p.hanusiak@workadventu.re"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecodingmachine/workadventure.git"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "prebuild": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node --transpileOnly ./src/index.ts",
    "start:dev": "ts-node-dev --inspect=0.0.0.0:9229 --exit-child --respawn ./src/index.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "precommit": "lint-staged",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "upload-test-map": "rm -f tests/assets.zip && cd tests/assets && zip -r ../assets.zip * && curl -F 'file=@../assets.zip' http://map-storage.workadventure.localhost/upload --basic -u john.doe:password",
    "push-sentry-sourcemaps": "tsc && sentry-cli releases new $SENTRY_RELEASE && sentry-cli releases files $SENTRY_RELEASE upload-sourcemaps dist && sentry-cli releases finalize $SENTRY_RELEASE"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.238.0",
    "@grpc/grpc-js": "^1.7.1",
    "@grpc/proto-loader": "^0.6.13",
    "@sentry/node": "^7.48.0",
    "@workadventure/map-editor": "1.0.0",
    "@workadventure/messages": "1.0.0",
    "@workadventure/shared-utils": "1.0.0",
    "@workadventure/tiled-map-type-guard": "^2.1.0",
    "archiver": "^5.3.1",
    "body-parser": "^1.20.1",
    "concurrently": "^7.3.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "fast-json-patch": "^3.1.1",
    "fs-extra": "^11.1.0",
    "google-protobuf": "^3.21.0",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "node-stream-zip": "^1.15.0",
    "nodemon": "^2.0.19",
    "npm-check-updates": "^16.0.5",
    "p-limit": "^3.1.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "zod": "^3.20.2",
    "zod-to-json-schema": "^3.20.4"
  },
  "devDependencies": {
    "@sentry/cli": "^2.17.2",
    "@types/archiver": "^5.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.186",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.11",
    "@types/passport-http": "^0.3.9",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.8.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "4.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
