{
  "name": "workadventureback",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc && cp -rf ./data ./dist/",
    "dev": "ts-node-dev --exit-child --respawn ./server.ts",
    "prod": "tsc && cp -rf ./data ./dist/ && node -r source-map-support/register --max-old-space-size=4096 ./dist/server.js",
    "runprod": "node -r source-map-support/register --max-old-space-size=4096 ./dist/server.js",
    "profile": "tsc && node --prof ./dist/server.js",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "lint": "DEBUG= node_modules/.bin/eslint src/ . --ext .ts",
    "fix": "DEBUG= node_modules/.bin/eslint --fix src/ . --ext .ts",
    "precommit": "lint-staged",
    "pretty": "yarn prettier --write 'src/**/*.{ts,tsx}'",
    "pretty-check": "yarn prettier --check 'src/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecodingmachine/workadventure.git"
  },
