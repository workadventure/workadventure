  "dependencies": {
    "@anatine/zod-openapi": "^1.3.0",
    "@grpc/grpc-js": "^1.6.7",
    "@xmpp/client": "^0.13.1",
    "@xmpp/debug": "^0.13.0",
    "axios": "^0.21.2",
    "cancelable-promise": "^4.2.1",
    "circular-json": "^0.5.9",
    "debug": "^4.3.1",
    "google-protobuf": "^3.13.0",
    "hyper-express": "^5.8.1",
    "jsonwebtoken": "^8.5.1",
    "mkdirp": "^1.0.4",
    "openapi3-ts": "^2.0.2",
    "openid-client": "^4.7.4",
    "prom-client": "^12.0.0",
    "qs": "^6.10.3",
    "query-string": "^6.13.3",
    "source-map-support": "^0.5.21",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-dist": "^4.5.1",
